dist: 
	@mkdir -p .tmp/xhtml2ssml && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/xhtml2ssml/ && ( cd `dirname .tmp/xhtml2ssml` && $(TAR) xhtml2ssml.tar xhtml2ssml && $(GZIP) xhtml2ssml.tar ) && $(MOVE) `dirname .tmp/xhtml2ssml`/xhtml2ssml.tar.gz . && $(DEL_FILE) -r .tmp/xhtml2ssml

